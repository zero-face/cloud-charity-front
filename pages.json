			"iconPath": "./static/img/split.png",
			"selectedIconPath": "static/img/splited.png",
			"text": "情感交流"
		},{
			"pagePath": "pages/user/user",
			"iconPath": "./static/img/user.png",
			"selectedIconPath": "static/img/usered.png",
